{
  "description": "Online learning made better",
  "version": "0.0.1",
  "manifest_version": 2,
  "name": "Edu-Pal",
  "author": "Markus Zhang, Arjun Barrett",
  "icons": {
    "16": "16.png",
    "24": "24.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "96": "96.png",
    "128": "128.png",
    "256": "256.png"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "256.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://meet.google.com/*"
      ],
      "js": [
        "injection.bundle.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.google.com/**-**-**"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "notifications",
    "*://meet.google.com/*"
  ],
  "web_accessible_resources": [
    "**/*.*.map"
  ],
  "content_security_policy": "script-src 'self' http://127.0.0.1:3000/; object-src 'self'"
}