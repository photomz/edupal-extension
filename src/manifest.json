{
  "manifest_version": 2,
  "name": "Edu-Pal",
  "version": "0.0.1",
  "author": "Markus Zhang, Arjun Barrett",
  "description": "Online learning made better",
  "icons": {
    "16": "extension/icons/16.png",
    "24": "extension/icons/24.png",
    "32": "extension/icons/32.png",
    "48": "extension/icons/48.png",
    "64": "extension/icons/64.png",
    "96": "extension/icons/96.png",
    "128": "extension/icons/128.png",
    "256": "extension/icons/256.png"
  },
  "background": {
    "scripts": ["extension/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "extension/icons/256.png",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": ["injected/index.ts"]
    }
  ],
  "externally_connectable": {
    "matches": ["*://meet.google.com/**-**-**"]
  },
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": true
  },
  "permissions": ["notifications", "*://meet.google.com/*"],
  "web_accessible_resources": ["**/*.*.map"]
}
