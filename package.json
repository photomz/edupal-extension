{
  "name": "edu-pal",
  "version": "0.0.1",
  "targets": {
    "dev": {
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "prod": {
      "sourceMap": false
    }
  },
  "targets.prod.alias": {
    "//": "not yet implemented in Parcel",
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils"
  },
  "description": "Google Meet client modifications with an emphasis on learning",
  "repository": "https://github.com/teachee/edu-pal",
  "contributors": [
    "Markus Zhang <markuszhang8@gmail.com>",
    "Arjun Barrett <arjunbarrett@gmail.com>"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "parcel src/manifest.json --target dev",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "build": "parcel build src/manifest.json --target prod"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "preact": "^10.5.5",
    "webextension-polyfill-ts": "^0.21.0"
  }
}
